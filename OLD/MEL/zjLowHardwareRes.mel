global proc zjLowHardwareRes()
{
	string $files[] = `ls -type file`;
	for ($item in $files)
	{
		catch(`addAttr -sn "resolution" -ln "resolution" -dv 32 -at "long" $item`);
		catch(`setAttr ($item + ".resolution") 32`);
	}
}