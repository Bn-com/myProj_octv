//pre Render Mel

global proc zjPreRender()
{
	//to avoid the collision between hsvAdjust.mll and MayaToAlias.mll,load hsvAdjust first!
	catch(`loadPlugin "HSVadjust2.mll"`);
	catch(`pluginInfo -edit -autoload true "HSVadjust2.mll"`);
	catch(`loadPlugin "HSVadjust2.mll"`);
}