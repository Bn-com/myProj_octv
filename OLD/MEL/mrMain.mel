global proc mrSetup_mushlar_e01(string $mushroom)
{
	int $count = `getAttr -size ($mushroom + ".vrts")`;

	if ($count != 1191)
	{
		warning($mushroom + " is not of type mushlar_e01.");
		return;
	}

	if (mrAlreadySetup($mushroom))
		return;


	string $type = "mushlar_e01";

	// create a group on top of the mushroom
	
	string $group = mrCreateGroup($mushroom);


	// create a GEO_group node same level as "group" node to store 
	// the locators, storing locators under MASTER_group causes problem

//	string $parents[] = `listRelatives -path -parent $group`;
//	string $GEO_group = `group -empty -name (mrShortName($mushroom) + "_GEO") -parent $parents[0]`;


	// update the name of $mushroom since it is now under a new group

	string $children[] = `listRelatives -path -children $group`;
	$mushroom = $children[0];


	// create the curve

	string $curve = mrCreateWireCurve($type, (mrShortName($mushroom) + "_curve"));

	// parent curve under group

	parent -relative $curve $group;

	// create wire deformer

	string $wire = mrCreateWire($curve, $mushroom);
	setAttr ($wire + ".dropoffDistance[0]") 100.0;
	mrPruneMembers(($wire + "Set"), $mushroom, $type);


	// create softbody

	mrCreateSoftBody(true, false, true, 1.0, false);

	// after softbody creation, the curve is unparented, re-parent it

	parent $curve $group;


	string $particle = $curve + "Particle";


	// assign per particle weight

	mrAssignParticleWeight($particle, $type);

	// create spring

	string $spring = mrCreateSpring($particle, (mrShortName($mushroom) + "_spring"));

	// parent spring
	
	parent $spring $group;


	// create target locator

	string $targetLocator = mrCreateLocator(mrShortName($mushroom) + "_targetLocator");

	// parent to group and turn off inheritTransform

	parent $targetLocator $group;
	setAttr ($targetLocator + ".inheritsTransform") false;

	// motion-path-constrain locator to wire curve (top end of curve)

	string $motionPath = `pathAnimation -fractionMode true -follow false -curve $curve $targetLocator`; // -followAxis x -upAxis y -worldUpType "vector" -worldUpVector 0 1 0 -inverseUp false -inverseFront false -bank false -curve $curve $targetLocator`;
	delete ($motionPath + "_uValue");		// delete the uValue animation node


	// create aim locator

	string $aimLocator = mrCreateLocator(mrShortName($mushroom) + "_aimLocator");

	// parent to group and turn off inheritTransform

	parent $aimLocator $group;
	setAttr ($aimLocator + ".inheritsTransform") false;

	// motion-path-constrain locator to wire curve (top end of curve)

	string $motionPath = `pathAnimation -fractionMode true -follow false -curve $curve $aimLocator`; //-followAxis y -upAxis z -worldUpType "vector" -worldUpVector 0 1 0 -inverseUp false -inverseFront false -bank false -curve $curve $aimLocator`;
	delete ($motionPath + "_uValue");		// delete the uValue animation node
	setAttr ($motionPath + ".uValue") 0.1;



	// set constraint to aimLocator's rotateX

	transformLimits -rotationX 0 0 -enableRotationX 1 1 $aimLocator;


	// aim constraint from targetLocator to aimLocator

	aimConstraint -name (mrShortName($mushroom) + "_aimConstraint") -weight 1 -aimVector 1 0 0 -upVector 0 1 0 -worldUpType "vector" -worldUpVector 0 1 0 $targetLocator $aimLocator;


	// create a cluster

//	select ($mushroom + ".vtx[0:405]") ($mushroom + ".vtx[424:427]") ($mushroom + ".vtx[437:440]") ($mushroom + ".vtx[450:453]") ($mushroom + ".vtx[463:466]") ($mushroom + ".vtx[485:488]") ($mushroom + ".vtx[498:501]") ($mushroom + ".vtx[511:514]") ($mushroom + ".vtx[524:527]") ($mushroom + ".vtx[537:540]") ($mushroom + ".vtx[550:553]") ($mushroom + ".vtx[650:666]") ($mushroom + ".vtx[669:682]") ($mushroom + ".vtx[686:689]") ($mushroom + ".vtx[692:701]") ($mushroom + ".vtx[704:709]") ($mushroom + ".vtx[714:717]") ($mushroom + ".vtx[736:751]") ($mushroom + ".vtx[774:798]") ($mushroom + ".vtx[849:899]") ($mushroom + ".vtx[932:941]") ($mushroom + ".vtx[952:955]") ($mushroom + ".vtx[958]") ($mushroom + ".vtx[961:1190]");
	select ($mushroom + ".vtx[0:405]") ($mushroom + ".vtx[424:427]") ($mushroom + ".vtx[437:440]") ($mushroom + ".vtx[450:453]") ($mushroom + ".vtx[463:466]") ($mushroom + ".vtx[485:488]") ($mushroom + ".vtx[498:501]") ($mushroom + ".vtx[511:514]") ($mushroom + ".vtx[524:527]") ($mushroom + ".vtx[537:540]") ($mushroom + ".vtx[550:557]") ($mushroom + ".vtx[562:565]") ($mushroom + ".vtx[570:573]") ($mushroom + ".vtx[578:581]") ($mushroom + ".vtx[586:589]") ($mushroom + ".vtx[594:597]") ($mushroom + ".vtx[602:605]") ($mushroom + ".vtx[610:613]") ($mushroom + ".vtx[618:621]") ($mushroom + ".vtx[626:629]") ($mushroom + ".vtx[634:637]") ($mushroom + ".vtx[642:645]") ($mushroom + ".vtx[650:666]") ($mushroom + ".vtx[668:682]") ($mushroom + ".vtx[684:689]") ($mushroom + ".vtx[691:702]") ($mushroom + ".vtx[704:710]") ($mushroom + ".vtx[712]") ($mushroom + ".vtx[714:718]") ($mushroom + ".vtx[728:751]") ($mushroom + ".vtx[774:798]") ($mushroom + ".vtx[849:1190]");
	string $clusters[] = `newCluster " -envelope 1"`;		// return cluster, clusterHandle

	// parent clusterHandle under group node

	parent $clusters[1] $group;

	// change the weighted node of clusterShape to the sphere

	string $clusterShapes[] = `listConnections -shapes true -source true -destination false ($clusters[0] + ".clusterXforms")`;

	if (size($clusterShapes) > 0)
		cluster -edit -bindState 1 -weightedNode $aimLocator $aimLocator $clusterShapes[0];

	// rename cluster handle

	rename $clusters[1] (mrShortName($mushroom) + "_clusterHandle");

	// lock attributes

	mrLockAttr($group);
//	mrLockAttr($GEO_group);
}

