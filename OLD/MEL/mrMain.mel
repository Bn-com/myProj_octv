global proc mrAdjustPos(string $object, string $curve, string $type)
{
	float $pos[] = `xform -q -objectSpace -scalePivot $object`;

	if ($type == "mushmed_d01_1")
	{
		xform -translation ($pos[0] - 2.03303) ($pos[1] - 0.0569763) ($pos[2] - (-1.979429)) $curve;
	}
	else
	if ($type == "mushmed_d01_2")
	{
		xform -translation ($pos[0] - (-0.542468)) ($pos[1] - 0.0) ($pos[2] - 0.774229) $curve;
	}
	else
	if ($type == "mushmed_d01_3")
	{
		xform -translation ($pos[0] - (-2.950618)) ($pos[1] - 0.0431595) ($pos[2] - 0.909866) $curve;
	}
	else
	if ($type == "mushmed_c01_1")
	{
		// according to pivot position in setup file
//		xform -translation ($pos[0] - (-0.539811)) ($pos[1] - (-0.0389179)) ($pos[2] - 2.856698) $curve;

		// according to pivot position in layout file
		xform -translation ($pos[0] - (-0.910995)) ($pos[1] - (-0.144044)) ($pos[2] - 3.002878) $curve;
	}
	else
	if ($type == "mushmed_c01_2")
	{
		// according to pivot position in setup file
//		xform -translation ($pos[0] - 0.0) ($pos[1] - 0.0) ($pos[2] - 0.0) $curve;
  
		// according to pivot position in layout file
		xform -translation ($pos[0] - 0.667997) ($pos[1] - (-0.126141)) ($pos[2] - (-2.261501)) $curve;
	}
	else
	if ($type == "mushmed_h01_1")
	{
		xform -translation ($pos[0] - 4.496487) ($pos[1] - 0.136001) ($pos[2] - (-0.485489)) $curve;
	}
	else
	if ($type == "mushmed_h01_2")
	{
		xform -translation ($pos[0] - 4.8675) ($pos[1] - 0.0180006) ($pos[2] - 0.574013) $curve;
	}
	else
	if ($type == "mushmed_h01_3")
	{
		xform -translation ($pos[0] - 4.005503) ($pos[1] - 0.0) ($pos[2] - (-3.880993)) $curve;
	}
	else
	if ($type == "mushmed_h01_4")
	{
		xform -translation ($pos[0] - 1.858002) ($pos[1] - 0.0350008) ($pos[2] - (-0.643982)) $curve;
	}
	else
	if ($type == "mushmed_h01_5")
	{
		xform -translation ($pos[0] - (-0.895493)) ($pos[1] - 0.0) ($pos[2] - 3.122002) $curve;
	}
	else
	if ($type == "mushmed_f01")
	{
//		xform -translation ($pos[0] - (-0.895493)) ($pos[1] - 0.0) ($pos[2] - 3.122002) $curve;
	}
	else
		error("Unknown mushroom type: " + $type);
}

global proc string[] mrCreateIK(string $type, string $name, string $startJoint, string $endJoint, string $solverType, string $curve)
{
	string $result[];

	if ($type == "mushmed_f01")
	{
		string $ikHandle[];

		if ($curve == "")
			$ikHandle = `ikHandle -name $name -numSpans 3 -startJoint $startJoint -endEffector $endJoint -solver $solverType`;
		else
			$ikHandle = `ikHandle -name $name -curve $curve -numSpans 3 -startJoint $startJoint -endEffector $endJoint -solver $solverType`;

		$result[0] = $ikHandle[0];		// IK handle
		$result[1] = $ikHandle[2];		// spline IK curve
	}
	else
	if ($type == "mushmed_d01_2")
	{
		string $ikHandle[];

		if ($solverType == "ikSplineSolver")
		{
			if ($curve == "")
				$ikHandle = `ikHandle -name $name -startJoint $startJoint -endEffector $endJoint -solver $solverType`;
			else
				$ikHandle = `ikHandle -name $name -curve $curve -createCurve false -simplifyCurve false -rootOnCurve false -parentCurve false -startJoint $startJoint -endEffector $endJoint -solver $solverType`;

			$result[0] = $ikHandle[0];		// IK handle
			$result[1] = $ikHandle[2];		// spline IK curve
		}
		else
		{
			// ikRPSolver type

			$ikHandle = `ikHandle -name $name -startJoint $startJoint -endEffector $endJoint -solver $solverType`;

			$result[0] = $ikHandle[0];		// IK handle
		}
	}
	else
	if (($type == "mushgro_a01") ||
	    ($type == "mushmed_h01_5"))
	{
		string $ikHandle[];

		if ($curve == "")
			$ikHandle = `ikHandle -name $name -startJoint $startJoint -endEffector $endJoint -solver $solverType`;
		else
			$ikHandle = `ikHandle -name $name -curve $curve -startJoint $startJoint -endEffector $endJoint -solver $solverType`;

		$result[0] = $ikHandle[0];		// IK handle
		$result[1] = $ikHandle[2];		// spline IK curve
	}
	else
	if (($type == "mushsma_s01_leaf") ||
	    ($type == "mushsma_m01"))
	{
		string $ikHandle[];

		if ($curve == "")
			$ikHandle = `ikHandle -name $name -numSpans 3 -startJoint $startJoint -endEffector $endJoint -solver $solverType`;
		else
			$ikHandle = `ikHandle -name $name -curve $curve -startJoint $startJoint -endEffector $endJoint -solver $solverType`;

		$result[0] = $ikHandle[0];		// IK handle
		$result[1] = $ikHandle[2];		// spline IK curve
	}
	else
	if ($type == "mushsma_s01_stem")
	{
		string $ikHandle[];

		if ($curve == "")
			$ikHandle = `ikHandle -name $name -numSpans 2 -startJoint $startJoint -endEffector $endJoint -solver $solverType`;
		else
			$ikHandle = `ikHandle -name $name -curve $curve -startJoint $startJoint -endEffector $endJoint -solver $solverType`;

		$result[0] = $ikHandle[0];		// IK handle
		$result[1] = $ikHandle[2];		// spline IK curve
	}
	else
		error("Unknown mushroom type: " + $type);

	return $result;
}

