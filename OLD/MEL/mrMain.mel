//
// mushmed_d01
//

global proc mrSetup_mushmed_d01(string $mushroom)
{
	if (mrAlreadySetup($mushroom))
		return;

	// differentiate 3 kinds of mushmed_d01 based on vertex count

	string $type = "mushmed_d01";

    int $count = `getAttr -size ($mushroom + ".vrts")`;

	if ($count == 821)
		$type = $type + "_1";
	else
		if ($count == 1005)
			$type = $type + "_2";
		else
			if ($count == 631)
				$type = $type + "_3";
			else
				error("Unknown mushmed_d01 type of vertex count: " + $count);

	if (($type == "mushmed_d01_1") || ($type == "mushmed_d01_3"))
	{
		// create a group on top of the mushroom
		
		string $group = mrCreateGroup($mushroom);

		// update the name of $mushroom since it is now under a new group

		string $children[] = `listRelatives -path -children $group`;
		$mushroom = $children[0];

		string $lattice = mrCreateLattice($type, mrShortName($mushroom) + "_"); 
		
		// parent lattice group under group

		parent ($lattice + "Group") $group;

		// create softbody

		mrCreateSoftBody(true, false, true, 1.0, false);
		setAttr ($lattice + "Particle.goalWeight[0]") 0.5;

		// after softbody creation, the lattice is unparented, re-parent it

		parent $lattice $group;


		string $particle = $lattice + "Particle";


		// assign per particle weight

		mrAssignParticleWeight($particle, $type);

		// create spring

		string $spring = mrCreateSpring($particle, (mrShortName($mushroom) + "_spring"));
		spring -e -stiffness 50 -damping 1.0 $spring;

		// parent spring
		
		parent $spring $group;

		// lock attributes

		mrLockAttr($group);	
	}
	else
	{
		// mushmed_d01_2

		string $GEO_group = `group -name (mrShortName($mushroom) + "_GEO") $mushroom`;
		$mushroom = $GEO_group + "|" + mrShortName($mushroom);


		// create a MASTER_group node same level as GEO_group node

		string $parents[] = `listRelatives -path -parent $GEO_group`;
		string $MASTER_group = `group -empty -name (mrShortName($mushroom) + "_MASTER") -parent $parents[0]`;


		// transfer transformation from mushroom to MASTER_group

		float $temp[] = `xform -q -relative -translation $mushroom`;
		xform -translation $temp[0] $temp[1] $temp[2] $MASTER_group;

		$temp = `xform -q -relative -rotation $mushroom`;
		xform -rotation $temp[0] $temp[1] $temp[2] $MASTER_group;

		$temp = `xform -q -relative -scale $mushroom`;
		xform -scale $temp[0] $temp[1] $temp[2] $MASTER_group;
		

		float $basePivot[3] = {-0.542468, 0, 0.774229};
		float $pivot[3] = `xform -q -objectSpace -rotatePivot $mushroom`;

		float $offset[3];

		$offset[0] = $pivot[0] - $basePivot[0];
		$offset[1] = $pivot[1] - $basePivot[1];
		$offset[2] = $pivot[2] - $basePivot[2];

		
		// create skeleton

		string $joints[] = mrCreateSkel($type, mrShortName($mushroom));

		// adjust position of skeleton

		move -relative $offset[0] $offset[1] $offset[2] $joints[0];

		// parent skeleton under MASTER_group node

		parent -relative $joints[0] $MASTER_group;


		// create IKs

		string $result[];
		
		$result = mrCreateIK($type, (mrShortName($mushroom) + "_ikHandle1"), $joints[0], $joints[2], "ikSplineSolver", "");
		parent $result[0] $MASTER_group;
		
		// rename IK spline curve 
		string $curve = `rename $result[1] (mrShortName($mushroom) + "_curve1")`;

		// create softbody
		select $curve;
		mrCreateSoftBody(true, false, true, 1.0, false);

		// after softbody creation, the curve is unparented, re-parent it
		parent $curve $MASTER_group;

		// assign per particle weight
		string $particle = $curve + "Particle";
		mrAssignParticleWeight($particle, $type + "_stem");


		$result = mrCreateIK($type, (mrShortName($mushroom) + "_ikHandle2"), $joints[3], $joints[5], "ikSplineSolver", "");
		parent $result[0] $MASTER_group;

		// rename IK spline curve 
		string $curve = `rename $result[1] (mrShortName($mushroom) + "_curve2")`;

		// create softbody
		select $curve;
		mrCreateSoftBody(true, false, true, 1.0, false);

		// after softbody creation, the curve is unparented, re-parent it
		parent $curve $MASTER_group;

		// assign per particle weight
		string $particle = $curve + "Particle";
		mrAssignParticleWeight($particle, $type + "_leaf");


		$result = mrCreateIK($type, (mrShortName($mushroom) + "_ikHandle3"), $joints[6], $joints[8], "ikSplineSolver", "");
		parent $result[0] $MASTER_group;

		// rename IK spline curve 
		string $curve = `rename $result[1] (mrShortName($mushroom) + "_curve3")`;

		// create softbody
		select $curve;
		mrCreateSoftBody(true, false, true, 1.0, false);

		// after softbody creation, the curve is unparented, re-parent it
		parent $curve $MASTER_group;

		// assign per particle weight
		string $particle = $curve + "Particle";
		mrAssignParticleWeight($particle, $type + "_leaf");


		$result = mrCreateIK($type, (mrShortName($mushroom) + "_ikHandle4"), $joints[9], $joints[11], "ikSplineSolver", "");
		parent $result[0] $MASTER_group;

		// rename IK spline curve 
		string $curve = `rename $result[1] (mrShortName($mushroom) + "_curve4")`;

		// create softbody
		select $curve;
		mrCreateSoftBody(true, false, true, 1.0, false);

		// after softbody creation, the curve is unparented, re-parent it
		parent $curve $MASTER_group;

		// assign per particle weight
		string $particle = $curve + "Particle";
		mrAssignParticleWeight($particle, $type + "_leaf");


		$result = mrCreateIK($type, (mrShortName($mushroom) + "_ikHandle5"), $joints[12], $joints[14], "ikSplineSolver", "");
		parent $result[0] $MASTER_group;

		// rename IK spline curve 
		string $curve = `rename $result[1] (mrShortName($mushroom) + "_curve5")`;

		// create softbody
		select $curve;
		mrCreateSoftBody(true, false, true, 1.0, false);

		// after softbody creation, the curve is unparented, re-parent it
		parent $curve $MASTER_group;

		// assign per particle weight
		string $particle = $curve + "Particle";
		mrAssignParticleWeight($particle, $type + "_leaf");


		// create spring

//		string $spring = mrCreateSpring($particle, (mrShortName($mushroom) + "_spring"));
//		parent $spring $MASTER_group;


