global proc mrSelectVineCVs(string $vine)
{
	string $shapes[] = `listRelatives -children -shapes $vine`;

	int $numCVs[] = `mrComputeNumCVs $shapes[0]`;

	int $numCVsU = $numCVs[0];
	int $numCVsV = $numCVs[1];

	select ($vine + ".cv[1:" + ($numCVsU - 2) + "][0:" + ($numCVsV - 1) + "]");
}

//
// mushvine_a01
//

global proc mrSetup_mushvine_a01(string $vine)
{
	if (mrAlreadySetup($vine))
		return;

	// create a GEO_group node on top of $vine

	string $GEO_group = `group -name (mrShortName($vine) + "_GEO") $vine`;
	$vine = $GEO_group + "|" + mrShortName($vine);

	// create a MASTER_group node same level as GEO_group node

	string $parents[] = `listRelatives -path -parent $GEO_group`;
	string $MASTER_group = `group -empty -name (mrShortName($vine) + "_MASTER") -parent $parents[0]`;

	// create sphere

	float $lowest[3] = mrComputeLowestPoint($vine);

//	print($lowest[0] + " " + $lowest[1] + " " + $lowest[2] + "\n");

	string $sphere[] = `sphere -name (mrShortName($vine) + "_rigidSphere") -pivot 0 0 0 -axis 0 1 0 -startSweep 0 -endSweep 360 -radius 1 -degree 3 -useTolerance 0 -tolerance 0.01 -sections 4 -spans 4 -constructionHistory false`;
	xform -translation $lowest[0] $lowest[1] $lowest[2] $sphere[0];

	parent $sphere[0] $MASTER_group;
	
	// create rigid body
	
	string $rigidBody = `rigidBody -active -name (mrShortName($vine) + "_rigidBody") -mass 10 -damping 0 -staticFriction 0.2 -dynamicFriction 0.2 -bounciness 0.6 -layer 0 -tesselationFactor 200 -initialVelocity 0 0 0 -initialAngularVelocity 0 0 0 -cache 0 -particleCollision 0 -impulse 0 0 0 -impulsePosition 0 0 0 -spinImpulse 0 0 0 -standInObject none`;

	// create rigid body nail constraint

	float $highest[3] = mrComputeHighestPoint($vine);
	string $constraint = `constrain -nail -name (mrShortName($vine) + "rigidNailConstraint") -position $lowest[0] $highest[1] $lowest[2]`;		// highest middle point

	// parent $constraint under MASTER_group node

	parent $constraint $MASTER_group;

	// create a cluster

	mrSelectVineCVs($vine);
	string $clusters[] = `newCluster " -envelope 1"`;		// return cluster, clusterHandle

	// change the weighted node of clusterShape to the sphere

	string $clusterShapes[] = `listConnections -shapes true -source true -destination false ($clusters[0] + ".clusterXforms")`;

	if (size($clusterShapes) > 0)
		cluster -edit -bindState 1 -weightedNode $sphere[0] $sphere[0] $clusterShapes[0];

	// parent clusterHandle under MASTER_group node

	parent $clusters[1] $MASTER_group;

	// hide sphere

//	hide $sphere[0];

	// rename cluster handle

	rename $clusters[1] (mrShortName($vine) + "_clusterHandle");

	// lock attributes

	mrLockAttr($MASTER_group);
	mrLockAttr($GEO_group);
}

//
// mushvine_b01
//

global proc mrSetup_mushvine_b01(string $vine)
{
	if (mrAlreadySetup($vine))
		return;

	// get bounding box information

	float $bbMin[3] = `getAttr ($vine + ".boundingBoxMin")`;
	float $bbMax[3] = `getAttr ($vine + ".boundingBoxMax")`;
	float $bbMid[3];

	$bbMid[0] = ($bbMin[0] + $bbMax[0]) / 2.0;
	$bbMid[1] = ($bbMin[1] + $bbMax[1]) / 2.0;
	$bbMid[2] = ($bbMin[2] + $bbMax[2]) / 2.0;

	float $length = $bbMax[1] - $bbMin[1];


	// create a group on top of the vine

	string $group = `group -name (mrShortName($vine) + "_MASTER") $vine`;
		
//	string $group = mrCreateGroup($vine);

	// update the name of $vine since it is now under a new group

	string $children[] = `listRelatives -path -children $group`;
	$mushroom = $children[0];


	// create sphere

	string $sphere1[] = `sphere -name (mrShortName($vine) + "_rigidSphere1") -pivot 0 0 0 -axis 0 1 0 -startSweep 0 -endSweep 360 -radius 0.2 -degree 3 -useTolerance 0 -tolerance 0.01 -sections 4 -spans 4 -constructionHistory false`;
	xform -translation $bbMid[0] ($bbMin[1] + (0.1 * $length)) $bbMid[2] $sphere1[0];

	parent -relative $sphere1[0] $group;
	
	// create rigid body
	
	string $rigidBody1 = `rigidBody -active -name (mrShortName($vine) + "_rigidBody1") -mass 1 -damping 0 -staticFriction 0.2 -dynamicFriction 0.2 -bounciness 0.6 -layer 0 -tesselationFactor 200 -initialVelocity 0 0 0 -initialAngularVelocity 0 0 0 -cache 0 -particleCollision 0 -impulse 0 0 0 -impulsePosition 0 0 0 -spinImpulse 0 0 0 -standInObject none`;


	string $sphere2[] = `sphere -name (mrShortName($vine) + "_rigidSphere2") -pivot 0 0 0 -axis 0 1 0 -startSweep 0 -endSweep 360 -radius 0.2 -degree 3 -useTolerance 0 -tolerance 0.01 -sections 4 -spans 4 -constructionHistory false`;
	xform -translation $bbMid[0] ($bbMin[1] + (0.7 * $length)) $bbMid[2] $sphere2[0];

	parent -relative $sphere2[0] $group;
	
	// create rigid body
	
	string $rigidBody2 = `rigidBody -active -name (mrShortName($vine) + "_rigidBody2") -mass 3 -damping 0 -staticFriction 0.2 -dynamicFriction 0.2 -bounciness 0.6 -layer 0 -tesselationFactor 200 -initialVelocity 0 0 0 -initialAngularVelocity 0 0 0 -cache 0 -particleCollision 0 -impulse 0 0 0 -impulsePosition 0 0 0 -spinImpulse 0 0 0 -standInObject none`;


	// create rigid body nail constraint

	string $constraint = `constrain -nail -name (mrShortName($vine) + "rigidNailConstraint") -position $bbMid[0] $bbMax[1] $bbMid[2] $sphere2[0]`;		

	// parent $constraint under group node

	parent -relative $constraint $group;


	// create rigid body pin constraint

	string $constraint = `constrain -pin -name (mrShortName($vine) + "rigidPinConstraint") -position $bbMid[0] ($bbMin[1] + (0.3 * $length)) $bbMid[2] $sphere1[0] $sphere2[0]`;

	// parent $constraint under group node

	parent -relative $constraint $group;


	// create a cluster

	select ($vine + ".vtx[0:113]");
	string $clusters[] = `newCluster " -envelope 1"`;		// return cluster, clusterHandle

	// change the weighted node of clusterShape to sphere2

	string $clusterShapes[] = `listConnections -shapes true -source true -destination false ($clusters[0] + ".clusterXforms")`;

	if (size($clusterShapes) > 0)
		cluster -edit -bindState 1 -weightedNode $sphere1[0] $sphere1[0] $clusterShapes[0];

	// parent clusterHandle under group node

	parent $clusters[1] $group;

	// hide spheres

//	hide $sphere1[0];
//	hide $sphere2[0];

	// rename cluster handle

	rename $clusters[1] (mrShortName($vine) + "_clusterHandle");

	// lock attributes

	mrLockAttr($group);
}

global proc mrSetup(string $type)
{	 
	string $selected[] = `ls -selection`;

	for ($item in $selected)
	{
		if ($type == "mushlar_c01")
			mrSetup_mushlar_c01($item);
		else
		if ($type == "mushlar_c02")
			mrSetup_mushlar_c02($item);
		else
		if ($type == "mushlar_a01")
			mrSetup_mushlar_a01($item);
		else
		if ($type == "mushlar_a02")
			mrSetup_mushlar_a02($item);
		else
		if ($type == "mushmed_d01")
			mrSetup_mushmed_d01($item);
		else
		if ($type == "mushmed_c01")
			mrSetup_mushmed_c01($item);
		else
		if ($type == "mushmed_h01")
			mrSetup_mushmed_h01($item);
		else
		if ($type == "mushmed_f01")
			mrSetup_mushmed_f01($item);
		else
		if ($type == "mushgro_a01")
			mrSetup_mushgro_a01($item);
		else
		if ($type == "mushlar_e01")
			mrSetup_mushlar_e01($item);
		else
		if ($type == "mushsma_t01")
			mrSetup_mushsma_t01($item);
		else
		if ($type == "mushsma_s01")
			mrSetup_mushsma_s01($item);
		else
		if ($type == "mushsma_s01_old")
			mrSetup_mushsma_s01_old($item);
		else
		if ($type == "mushsma_m01")
			mrSetup_mushsma_m01($item);
		else
		if ($type == "mushflo_a02")
			mrSetup_mushflo_a02($item);
		else
		if ($type == "mushflo_a01")
			mrSetup_mushflo_a01($item);
		else
		if ($type == "mushvine_a01")
			mrSetup_mushvine_a01($item);
		else
		if ($type == "mushvine_b01")
			mrSetup_mushvine_b01($item);
		else
			error("Unknown mushroom type: " + $type);
	}
}

