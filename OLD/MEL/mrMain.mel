//
// mushsma_s01
//

global proc mrSetup_mushsma_s01_old(string $mushroom)
{
	int $count = `getAttr -size ($mushroom + ".vrts")`;

	if ($count != 8469)
	{
		warning($mushroom + " is not of type mushsma_s01.");
		return;
	}

	if (mrAlreadySetup($mushroom))
		return;

	string $type = "mushsma_s01";

	// create a group on top of the mushroom
	
	string $group = mrCreateGroup($mushroom);

	// update the name of $mushroom since it is now under a new group

	string $children[] = `listRelatives -path -children $group`;
	$mushroom = $children[0];


	// create a torus as wrap deformer object

	string $torus[] = `polyTorus -name (mrShortName($mushroom) + "_torus") -radius 2.5 -sectionRadius 1 -twist 0 -subdivisionsX 8 -subdivisionsY 4 -axis 0 1 0 -texture 0 -constructionHistory off`;
	xform -translation 0 3.842 0 $torus[0];
	xform -scale 1 1.50609856 1 $torus[0];

	// parent torus under group

	parent -relative $torus $group;

	// create wrap deformer

	select $mushroom $torus[0];
	string $wrap[] = `doWrapArgList "2" { "1","0","0.0" }`;
	mrPruneMembers(($wrap[0] + "Set"), $mushroom, $type);


	// create softbody

	select $torus[0];
	mrCreateSoftBody(true, false, true, 1.0, false);

	// after softbody creation, the torus is unparented, re-parent it

	parent $torus[0] $group;


	string $particle = $torus[0] + "Particle";


	// assign per particle weight

	mrAssignParticleWeight($particle, $type);

	// create spring

	string $spring = mrCreateSpring($particle, (mrShortName($mushroom) + "_spring"));

	// parent spring
	
	parent $spring $group;

	// hide torus

	hide $torus[0];

	// lock attributes

	mrLockAttr($group);
}

