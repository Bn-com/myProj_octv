global proc string[] mrCreateSkinCluster(string $joint, string $mesh)
{
	string $selected[] = `ls -selection`;

	select $joint $mesh;
	string $result[] = `newSkinCluster "-maximumInfluences 5 -dropoffRate 4"`;

	// restore original selection

	select $selected;

	return $result;
}

proc mrSetRandomKey(string $obj, string $attr, float $min, float $max, int $spacing)
{
	float $start = `playbackOptions -q -min`;
	float $end = `playbackOptions -q -max`;

	for ($i = $start; $i < $end; $i += $spacing)
	{
		float $rand = `rand $min $max`;
		setKeyframe -time $i -value $rand -attribute $attr $obj;
	}
}

global proc string mrCreateSpring(string $particle, string $springName)
{
	// create spring

	string $springs[] = `spring -name $springName -noDuplicate false -minMax true -minDistance 0 -maxDistance 0 -useRestLengthPS true -stiffness 1 -damping 0.2 -startForceWeight 1 -endForceWeight 1 $particle`;

	return $springs[0];
}

global proc string mrCreateLattice(string $type, string $latticeName)
{
	string $latticeNames[];

//	if ($type == "mushmed_h01_5")
//		$latticeNames = `lattice -name $latticeName -divisions 5 5 3 -commonParent -objectCentered true -ldivisions 2 2 2`;
//	else
	if ($type == "mushmed_d01_1")
		$latticeNames = `lattice -name $latticeName -divisions 2 5 2 -commonParent -objectCentered true -ldivisions 2 2 2`;
	else
	if ($type == "mushmed_d01_3")
		$latticeNames = `lattice -name $latticeName -divisions 2 3 2 -commonParent -objectCentered true -ldivisions 2 2 2`;
	else
