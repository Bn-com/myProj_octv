global proc mrSetup_mushmed_f01(string $mushroom)
{
	string $children[] = `listRelatives -path -children $mushroom`;

	if (`size $children` != 6)
		error("Select the group node of mushmed_f01 (The group node should have 6 meshes).");

	if (mrAlreadySetup($mushroom))
		return;

	string $type = "mushmed_f01";


	// create a GEO_group node on top of $mushroom

	string $GEO_group = `group -name (mrShortName($mushroom) + "_GEO") $mushroom`;
	$mushroom = $GEO_group + "|" + mrShortName($mushroom);


	// create a MASTER_group node same level as GEO_group node

	string $parents[] = `listRelatives -path -parent $GEO_group`;
	string $MASTER_group = `group -empty -name (mrShortName($mushroom) + "_MASTER") -parent $parents[0]`;
	

	// transfer transformation from mushroom to MASTER_group

	float $temp[] = `xform -q -relative -translation $mushroom`;
	xform -translation $temp[0] $temp[1] $temp[2] $MASTER_group;

	$temp = `xform -q -relative -rotation $mushroom`;
	xform -rotation $temp[0] $temp[1] $temp[2] $MASTER_group;

	$temp = `xform -q -relative -scale $mushroom`;
	xform -scale $temp[0] $temp[1] $temp[2] $MASTER_group;


	// create skeleton and spline IK

	string $result[] = mrCreateSkel($type, mrShortName($mushroom));

	string $startJoint = $result[0];
	string $endJoint = $result[size($result) - 1];

	// adjust position of skeleton

//	mrAdjustPos($group, $startJoint, $type);

	// parent skeleton under MASTER_group node

	parent -relative $startJoint $MASTER_group;

	$result = mrCreateIK($type, (mrShortName($mushroom) + "_ikHandle"), $startJoint, $endJoint, "ikSplineSolver", "");

	string $IK = $result[0];
	string $curve = $result[1];

	// parent IK under MASTER_group node

	parent $IK $MASTER_group;

	// create skinCluster for each mesh

	for ($i = 0; $i < size($children); $i++)
		mrCreateSkinCluster($startJoint, $children[$i]);

	
	// create softbody

	select $curve;
	mrCreateSoftBody(true, false, true, 1.0, false);

	// after softbody creation, the curve is unparented, re-parent it

	parent $curve $MASTER_group;


	string $particle = $curve + "Particle";


	// assign per particle weight

	mrAssignParticleWeight($particle, $type);

	// create spring

	string $spring = mrCreateSpring($particle, (mrShortName($mushroom) + "_spring"));

	// parent spring
		
	parent $spring $MASTER_group;

	// lock attributes

	mrLockAttr($MASTER_group);
	mrLockAttr($GEO_group);
}

