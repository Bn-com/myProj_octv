global proc string mrCreateWire(string $curve, string $object)
{
	string $wire[] = `wire -envelope 1.000000 -crossingEffect 0.000000 -localInfluence 0.000000 -wire $curve $object`;

	return $wire[0];
}

global proc mrCreateSoftBody(int $hide, int $history, int $goal, float $goalWeight, int $makeCopySoft)
{
	// create softbody

	dynCreateSoft $hide $history $goal $goalWeight $makeCopySoft;
}

global proc string mrCreateLocator(string $name)
{
	// create locator

	string $locator[] = `spaceLocator`;
	string $name = `rename $locator[0] $name`;

	return $name;
}

