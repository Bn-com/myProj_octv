global proc int[] mrComputeNumCVs(string $nurbsShape)
{
	int $numSpansU = `getAttr ($nurbsShape + ".spansU")`;
    int $degreeU = `getAttr ($nurbsShape + ".degreeU")`;

    int $numSpansV = `getAttr ($nurbsShape + ".spansV")`;
    int $degreeV = `getAttr ($nurbsShape + ".degreeV")`;

    int $formU = `getAttr ($nurbsShape + ".formU")`;
    int $formV = `getAttr ($nurbsShape + ".formV")`;

    int $numCVsU = $numSpansU + $degreeU;

    // Adjust for periodic hull
    
	if ($formU == 2) 
		$numCVsU -= $degreeU;

    int $numCVsV = $numSpansV + $degreeV;

    // Adjust for periodic hull

    if ($formV == 2) 
		$numCVsV -= $degreeV;

	int $result[];

	$result[0] = $numCVsU;
	$result[1] = $numCVsV;

	return $result;
}

global proc string mrCreateCurve(string $shape)
{
	int $numCVs[] = `mrComputeNumCVs $shape`;

	int $numCVsU = $numCVs[0];
	int $numCVsV = $numCVs[1];

	if ($numCVsU < 4)
	{
		warning("vine should have more than 3 U points");
		return "";
	}

	float $curve[];
	clear $curve;

	for ($i = 0; $i < $numCVsU; $i++)
	{
		float $avg[];
		clear $avg;

		for ($j = 0; $j < $numCVsV; $j++)
		{
			float $pos[] = `xform -worldSpace -q -translation ($shape + ".cv[" + $i + "][" + $j + "]")`;
			
			$avg[0] += $pos[0];
			$avg[1] += $pos[1];
			$avg[2] += $pos[2];
		}

		$curve[$i * 3 + 0] = $avg[0] / $numCVsV;
		$curve[$i * 3 + 1] = $avg[1] / $numCVsV;
		$curve[$i * 3 + 2] = $avg[2] / $numCVsV;
	}

	// create the curve

	string $cmd = "curve -d 3 ";

	for ($i = 0; $i < size($curve); $i += 3)
		$cmd += "-p " + $curve[$i] + " " + $curve[$i + 1] + " " + $curve[$i + 2] + " ";

	int $k = 0;
	int $numKnots = (size($curve) / 3) + 2;			// degree 3 curve

	for ($i = 0; $i < 3; $i++)
		$cmd += " -k " + $k;

	for (; $i < $numKnots - 3; $i++)
		$cmd += " -k " + (++$k);

	for (; $i < $numKnots; $i++)
		$cmd += " -k " + ($k + 1);

	return (eval($cmd));
}

global proc float[] mrComputeLowestPoint(string $vine)
{
	string $shapes[] = `listRelatives -children -shapes $vine`;

	float $point[3];

	string $curve = mrCreateCurve($shapes[0]);

	if ($curve != "")
	{
		// compute lowest point

		string $curveShapes[] = `listRelatives -children $curve`;
		$point = `curveLowestPointCmd ($curveShapes[0])`;

		delete $curve;
	}

	return $point;
}

global proc float[] mrComputeHighestPoint(string $vine)
{
	string $shapes[] = `listRelatives -children -shapes $vine`;

	int $numCVs[] = `mrComputeNumCVs $shapes[0]`;

	int $numCVsU = $numCVs[0];
	int $numCVsV = $numCVs[1];

	float $maxY = -10000.0;
	float $highest[3];

	for ($j = 0; $j < $numCVsV; $j++)
	{
		float $pos[] = `xform -worldSpace -q -translation ($shapes[0] + ".cv[" + 0 + "][" + $j + "]")`;
			
		if ($pos[1] > $maxY)
		{
			$highest[0] = $pos[0];
			$highest[1] = $pos[1];
			$highest[2] = $pos[2];

			$maxY = $pos[1];
		}
	}

	for ($j = 0; $j < $numCVsV; $j++)
	{
		float $pos[] = `xform -worldSpace -q -translation ($shapes[0] + ".cv[" + ($numCVsU - 1) + "][" + $j + "]")`;
			
		if ($pos[1] > $maxY)
		{
			$highest[0] = $pos[0];
			$highest[1] = $pos[1];
			$highest[2] = $pos[2];

			$maxY = $pos[1];
		}
	}

	return $highest;
}

global proc mrSelectVtxs(string $mushroom, string $type)
{
