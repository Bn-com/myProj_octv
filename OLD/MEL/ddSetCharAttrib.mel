  $temp = `joint -q -p ($charName + "_jaw")` ;
  //xform -rp $temp[0] $temp[1] $temp[2] ($charName + "_jaw_driven") ;
  move $temp[0] $temp[1] $temp[2] ($charName + "_jaw_driven.scalePivot") ($charName + "_jaw_driven.rotatePivot") ;

