global proc yuImportGeoCache(string $path)
{
	file -newFile;
	file -rename $path;
	eval "source zwSceneOpenedScriptJob.mel";
	catch(`zwSceneOpened`);
	source "yuImportGeoTool.mel";
	writeXML();
	importGeoCache();

	file -save;
}
