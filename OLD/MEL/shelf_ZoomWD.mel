global proc ZoomWDShelfChanges(string $steps)
{
	//这里的ZoomWD和zwShelf里的第一行项目名一致
	string $childArray[] = `shelfLayout -query -childArray ZoomWD`;
	int $size = size($childArray);

	int $index = 7;

	for ($i=0; $i<$size; $i++)
	{
		string $docTag = `shelfButton -query -docTag $childArray[$i]`;
		if ($docTag != "")
		{
			if ($docTag == $steps)
			{
				shelfButton -edit -visible 1 $childArray[$i];
				$index ++;
				shelfLayout -edit -position $childArray[$i] $index ZoomWD;
			}
			else
			{
				shelfButton -edit -visible 0 $childArray[$i];
			}
		}
	}
	optionVar -stringValue "ZoomWDShelfSelected" $steps;
}


