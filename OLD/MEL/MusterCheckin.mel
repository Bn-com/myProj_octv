global proc zwMusterMayaManUpdateCustomShader()
{
	if (!`pluginInfo -query -loaded "MayaMan"`)
	{
		return;
	}

	string $MayaManCustomShaders[] = `ls -type "MayaManCustomShader"`;
	for ($MayaManCustomShader in $MayaManCustomShaders)
	{
		if (`objExists ($MayaManCustomShader + ".maxdisp")`)
		{
			continue;
		}
		string $ShaderFile = `getAttr ($MayaManCustomShader + ".ShaderFile")`;
		if ($ShaderFile == "")
		{
			continue;
		}
		catch(`evalEcho ("MayaManUpdateCustomShader \"" + $MayaManCustomShader + "\" \"" + $ShaderFile + "\"")`);
	}
}

global proc int[] zwMusterQueryJobs()
{
	string $server = `optionVar -query musterCheckinServer`;

	string $mrtool = zwMusterGetMrtool() + "\\Mrtool.exe";
	if ($server == "192.168.3.185")
	{
		$mrtool = zwMusterGetMrtool() + "_185\\Mrtool.exe";
	}
	else if ($server == "192.168.3.181")
	{
		$mrtool = zwMusterGetMrtool() + "_181\\Mrtool.exe";
	}
	else if ($server == "192.168.3.182")
	{
		$mrtool = zwMusterGetMrtool() + "_182\\Mrtool.exe";
	}
	else if ($server == "192.168.168.183")
	{
		$mrtool = zwMusterGetMrtool() + "_183\\Mrtool.exe";
	}

	int $sf = `optionVar -query musterCheckinStart`;
	int $ef = `optionVar -query musterCheckinEnd`;

	int $jobs[];
	clear $jobs;

	string $jobnames[];
	clear $jobnames;
	if (`optionVar -query "musterCheckinLayer"`)
	{
		string $renderLayers[] = `listConnections "renderLayerManager.renderLayerId"`;
		for ($renderLayer in $renderLayers)
		{
			$jobnames[size($jobnames)] = zwMusterCheckinGetJobName($renderLayer);
		}
	}
	else
	{
		$jobnames[size($jobnames)] = zwMusterCheckinGetJobName("");
	}

	string $cmd = "\"" + $mrtool + "\" -s " + $server + " -u admin -p 110 -q j -jf id,name,status,sf,ef";
	string $rs = `system $cmd`;
	string $lines[];
	tokenize $rs "\n" $lines;
	for ($line in $lines)
	{
		string $fields[];
		if (`tokenize $line "|" $fields` != 6)
		{
			continue;
		}

		int $find = false;
		string $name = strip($fields[1]);
		for ($i=0; $i<size($jobnames); $i++)
		{
			if ($name == $jobnames[$i])
			{
				$find = true;
				break;
			}
		}
		if (!$find)
		{
			continue;
		}

		string $status = strip($fields[2]);
		if (`match "Completed" $status` != "")
		{
			continue;
		}

		int $sf1 = int(strip($fields[3]));
		int $ef1 = int(strip($fields[4]));
		if (!($sf == $sf1 && $ef == $ef1))
		{
			continue;
		}

		int $id = int(strip($fields[0]));
		$jobs[size($jobs)] = $id;
	}
	return $jobs;
}

global proc zwMusterDeleteJobs(int $ids[])
{
	string $server = `optionVar -query musterCheckinServer`;

	string $mrtool = zwMusterGetMrtool() + "\\Mrtool.exe";
	if ($server == "192.168.3.185")
	{
		$mrtool = zwMusterGetMrtool() + "_185\\Mrtool.exe";
	}
	else if ($server == "192.168.3.181")
	{
		$mrtool = zwMusterGetMrtool() + "_181\\Mrtool.exe";
	}
	else if ($server == "192.168.3.182")
	{
		$mrtool = zwMusterGetMrtool() + "_182\\Mrtool.exe";
	}
	else if ($server == "192.168.168.183")
	{
		$mrtool = zwMusterGetMrtool() + "_183\\Mrtool.exe";
	}

	for ($id in $ids)
	{
		string $cmd = "\"" + $mrtool + "\" -s " + $server + " -u admin -p 110 -jobrm " + $id;
		system $cmd;
	}
}

global proc zwMusterL2FileCluster()
{
	string $files[] = `ls -type "file"`;
	for ($file in $files)
	{
		string $fileTextureName = `getAttr ($file + ".fileTextureName")`;
		string $str = `substitute "^L:" $fileTextureName "//file-cluster/GDC"`;
		if ($fileTextureName != $str)
		{
			evalEcho ("setAttr \"" + $file + ".fileTextureName\" -type \"string\" \"" + $str + "\"");
		}
	}
}