global proc int zwMusterPacket()
{
	global int $zwMusterCheckinPacket;	// 外部工具批量提交时指定了packet
	if ($zwMusterCheckinPacket != 0)
	{
		return $zwMusterCheckinPacket;
	}

	int $packet = 1;

	string $sceneName = `file -query -sceneName -shortName`;
	string $project = zwMusterGetProject($sceneName);
	string $str = `python ("idmt.pipeline.project.GetSetting('" + $project + "', 'muster/packet')")`;
	if ($str != "")
	{
