global proc zwMusterPoolMenu(string $popupMenu, string $textFieldGrp)
{
	string $pools[];
	string $dispatcher = `textFieldGrp -query -text dispatcherCTL`;
	if ($dispatcher == "192.168.3.181")
	{
		$pools = {"3dlight"};
	}
	else if ($dispatcher == "192.168.3.182")
	{
		$pools = {"P5", "3Dlight", "LC", "arnold", "MAYA2014"};
	}
	else if ($dispatcher == "192.168.3.185")
	{
		$pools = {"Entire Farm","test", "MA_test", "4instances"};
	}
	else if ($dispatcher == "192.168.168.183")
	{
		$pools = {"toCloud"};
	}
	$pools = sort($pools);
	stringArrayInsertAtIndex(0, $pools, "Entire Farm");

	popupMenu -edit -deleteAllItems $popupMenu;
	setParent -menu $popupMenu;
	for ($pool in $pools)
	{
		menuItem -label $pool -command ("textFieldGrp -edit -text \"" + $pool + "\" \"" + $textFieldGrp + "\"; OnTextFieldGrpDispatcherChanged 2;");
	}
}

global proc zwMusterServerMenu(string $popupMenu, string $textFieldGrp)
{
	setParent -menu $popupMenu;

	string $buf[];
	clear $buf;
	if (`getenv "OFFICE_LOCATION"` == "chongqing")
	{
		$buf = {"192.168.23.23"};
	}
	else if (`getenv "OFFICE_LOCATION"` == "shenzhen")
	{
		$buf = {"192.168.3.181", "192.168.3.182", "192.168.3.185", "192.168.168.183"};
	}
	for ($server in $buf)
	{
		menuItem -label $server -command ("textFieldGrp -edit -text \"" + $server + "\" \"" + $textFieldGrp + "\"; OnTextFieldGrpDispatcherChanged 1;");
	}
}

global proc OnTextFieldGrpDispatcherChanged(int $ctrl)
{
	string $project = zwMusterGetProject("");
	//if (($project == "ChangBaiShan" || $project == "EarthQuake") || $project == "TomAndJerry")
	{
		string $projectFolder;
		string $musterCheckinOut = GetOutputPathByName();

		string $dispatcher = `textFieldGrp -query -text dispatcherCTL`;
		if ($dispatcher == "192.168.168.183" && $project != "Strawberry")// && (($project == "ChangBaiShan" || $project == "EarthQuake") || $project == "TomAndJerry"))
		{
			string $date;
			string $musterCheckinPool = `textFieldGrp -query -text poolCTL`;
			if ($musterCheckinPool == "hanhong")
			{
				$date = "hanhong";
			}
			else
			{
				if ($ctrl != 2)
				{
					$musterCheckinPool = "toCloud";
				}
				python "import datetime";
				$date = `python "(datetime.datetime.now() + datetime.timedelta(0, 0, 0, 0, 0, 7)).strftime('%Y%m\\%Y%m%d')"`;
			}
			$projectFolder = "\\\\file-cluster\\GDC\\Netrender\\Maya_Odd\\cloud\\" + $date;

			$musterCheckinOut = `substitute $project $musterCheckinOut ("cloud\\" + $project)`;

			optionVar -stringValue musterCheckinPool $musterCheckinPool;
			textFieldGrp -edit -text $musterCheckinPool poolCTL;

		//	if ($project == "VickyTheViking")
		//	{
		//		intFieldGrp -edit -value1 40 priCTL;
		//	}
		}
		else
		{
			$projectFolder = zwGetMusterProject("");

		//	if ($project == "VickyTheViking")
		//	{
		//		intFieldGrp -edit -value1 90 priCTL;
		//	}
			if ($project == "VickyTheViking" || $project == "Ninjago" )
			{
				string $musterCheckinPool = `textFieldGrp -query -text poolCTL`;
				if ($ctrl != 2)
				{
					$musterCheckinPool = "Entire Farm";
				}
				optionVar -stringValue musterCheckinPool $musterCheckinPool;
				textFieldGrp -edit -text $musterCheckinPool poolCTL;
			}
		}
		optionVar -stringValue musterCheckinProject $projectFolder;
		textFieldGrp -edit -text $projectFolder projpathCTL;

		optionVar -stringValue "musterCheckinOut" $musterCheckinOut;
		textFieldButtonGrp -edit -text $musterCheckinOut OutPutPathCTL;
	}
}

