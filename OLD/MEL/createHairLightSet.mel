global proc createHairLightSet(){	$keyLight = `spotLight -n Key_Light`;	$keyLightShape = `ls -sl -dag -type shape`;	setAttr ($keyLightShape[0] + ".intensity") 0.8;	setAttr ($keyLightShape[0] + ".useDepthMapShadows") 1;	setAttr ($keyLightShape[0] + ".dmapResolution") 512;	setAttr ($keyLightShape[0] + ".dmapFilterSize") 2;	setAttr ($keyLightShape[0] + ".dmapBias") 0.25;	setAttr ($keyLightShape[0] + ".shadowColor") -type double3 0.2 0.2 0.2 ;	setAttr "Key_Light.translateX" -154.47;	setAttr "Key_Light.translateY" 258.327;	setAttr "Key_Light.translateZ" 160.183 ;	setAttr "Key_Light.rotateX" -36;	setAttr "Key_Light.rotateY" -42.4;	$fillLight = `spotLight -n Fill_Light`;	$fillLightShape = `ls -sl -dag -type shape`;	setAttr ($fillLightShape[0] + ".emitSpecular") 0;	setAttr ($fillLightShape[0] + ".intensity") 0.2;	setAttr ($fillLightShape[0] + ".useDepthMapShadows") 1;	setAttr ($fillLightShape[0] + ".dmapResolution") 256;	setAttr ($fillLightShape[0] + ".dmapFilterSize") 4;	setAttr ($fillLightShape[0] + ".dmapBias") 0.25;	setAttr ($fillLightShape[0] + ".shadowColor") -type double3 0.2 0.2 0.2 ;	setAttr "Fill_Light.translateX" 179.542 ;	setAttr "Fill_Light.translateY" 190.421 ;	setAttr "Fill_Light.translateZ" 194.437 ;	setAttr "Fill_Light.rotateX" -19.8;	setAttr "Fill_Light.rotateY" 42;	$rimLight = `spotLight -n Rim_Light`;	$rimLightShape = `ls -sl -dag -type shape`;	setAttr ($rimLightShape[0] + ".emitSpecular") 0;	setAttr ($rimLightShape[0] + ".intensity") 0.6;	setAttr ($rimLightShape[0] + ".useDepthMapShadows") 1;	setAttr ($rimLightShape[0] + ".dmapResolution") 256;	setAttr ($rimLightShape[0] + ".dmapFilterSize") 4;	setAttr ($rimLightShape[0] + ".dmapBias") 0.25;	setAttr ($rimLightShape[0] + ".shadowColor") -type double3 0.2 0.2 0.2 ;	setAttr "Rim_Light.translateX" 132.372 ;	setAttr "Rim_Light.translateY" 279.08 ;	setAttr "Rim_Light.translateZ" -184.94 ;	setAttr "Rim_Light.rotateX" -39;	setAttr "Rim_Light.rotateY" 144;	group -n LIGHT_Set Key_Light Fill_Light Rim_Light;	//select -r Key_Light Fill_Light Rim_Light ;	//MayaManAddLightAttributes(true, true);	select -r Key_Light ;	MayaManAddLightAttributes(true, true);       rename MayaManLightAtts1 "MM_hair_KeyLight" ;	setAttr "MM_hair_KeyLight.mmla_DeepShadows" 1;	setAttr "MM_hair_KeyLight.mmla_VolumeInterp" 1;	setAttr "MM_hair_KeyLight.mmla_PixelSamplesX" 8;	setAttr "MM_hair_KeyLight.mmla_PixelSamplesY" 8;       setAttr -type "string" MM_hair_KeyLight.mmla_Categories "Key";	select -r Fill_Light ;	MayaManAddLightAttributes(true, true);       rename MayaManLightAtts1 "MM_hair_FillLight" ;	setAttr "MM_hair_FillLight.mmla_DeepShadows" 1;	setAttr "MM_hair_FillLight.mmla_VolumeInterp" 1;	setAttr "MM_hair_FillLight.mmla_PixelSamplesX" 8;	setAttr "MM_hair_FillLight.mmla_PixelSamplesY" 8;       setAttr -type "string" MM_hair_FillLight.mmla_Categories "Fill";	select -r Rim_Light ;	MayaManAddLightAttributes(true, true);       rename MayaManLightAtts1 "MM_hair_RimLight" ;	setAttr "MM_hair_RimLight.mmla_DeepShadows" 1;	setAttr "MM_hair_RimLight.mmla_VolumeInterp" 1;	setAttr "MM_hair_RimLight.mmla_PixelSamplesX" 8;	setAttr "MM_hair_RimLight.mmla_PixelSamplesY" 8;       setAttr -type "string" MM_hair_RimLight.mmla_Categories "Rim";} 