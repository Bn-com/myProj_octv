global proc pyAnimCrowd()
{
	if (`window -ex pyAnimCrowdUI`)
	{
		deleteUI pyAnimCrowdUI;
	}

	window -t "Crowd Tools" pyAnimCrowdUI;
	showWindow pyAnimCrowdUI;
}