global proc string  s100capitalizeFirstLetter(string $name)
{
$first = startString($name, 1) ;
$noFirst = substring($name, 2 , 10000) ;
$cap = capitalizeString($first ) ;
$complete = ($cap + $noFirst );
return $complete;

}


