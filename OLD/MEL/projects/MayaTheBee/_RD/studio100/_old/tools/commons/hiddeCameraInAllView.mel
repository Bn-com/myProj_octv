global proc hiddeCameraInAllView()
{


$test = `getPanel -typ "modelPanel" `;

	for ( $testT in $test)
	{
	modelEditor -e -cameras 0 $testT;


    }


}

global proc showCameraInAllView()
{


$test = `getPanel -typ "modelPanel" `;

	for ( $testT in $test)
	{
	modelEditor -e -cameras 1 $testT;


    }


}