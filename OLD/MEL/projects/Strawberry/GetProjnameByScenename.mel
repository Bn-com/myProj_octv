global proc string GetProjnameByScenename(string $sn)
{
	string $pn;

	switch ($sn)
	{
		case "wi" :
			$pn = "WinxClub";
			break;
		case "bu" :
			$pn = "BubbleGuppies";
			break;
		case "rr" :
			$pn = "RainbowRiders";
			break;
		case "sk" :
			$pn = "Strawberry";
			break;
		case "wx" :
			$pn = "WinxClubII";
			break;
		case "ey" :
			$pn = "Enyo";
			break;
		case "wn2" :
			$pn = "WinxII";
			break;
	}
	return $pn;
}