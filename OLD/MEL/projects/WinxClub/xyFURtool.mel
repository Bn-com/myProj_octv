global proc xyFURtool() {	if(`window -exists xyFURtoolUI`)		deleteUI xyFURtoolUI;	window -title "WINX FURCACHE TOOL"               -width 200 -height 360  xyFURtoolUI;    scrollLayout  -w 200;        columnLayout -adjustableColumn true -cw 160;                          button -l "wi_c014001_chirone_h_hr.mb" -w 200                      -c ("FURimport(\"c014001\",\"chirone\")");                                                       button -l "wi_c026001_cornelius_h_hr.mb"                     -c ("FURimport(\"c026001\",\"cornelius\")");              button -l "wi_c022001_frankino_h_hr.mb"                     -c ("FURimport(\"c022001\",\"frankino\")");              button -l "wi_c025001_mule_h_hr.mb"                     -c ("FURimport(\"c025001\",\"mule\")");              button -l "wi_c023001_smilzo_h_hr.mb"                     -c ("FURimport(\"c023001\",\"smilzo\")");                          button -l "wi_c023001_smilzohair_h_hr.mb"                     -c ("FURimport(\"c023001\",\"smilzohair\")");       showWindow xyFURtoolUI;}global proc FURimport(string $id,string $chrname){ source doImportCacheArgList.mel;string $namespace = $id+"_"+$chrname+"_fur_for_cache";string $path = "//file-cluster/GDC/Projects/WinxClub/Project/scenes/characters/"+$id+"/hair/wi_"+$id+"_"+$chrname+"_h_hr.mb";file -import -type "mayaBinary" -ra true -namespace $namespace -options "v=0"  -pr -loadReferenceDepth "all" $path;//++++++++get geocache path  string $path = `file -q -sceneName`;string $base = basename($path,".mb");string $filenamearray[];$filenamearray = stringToStringArray($base, "_");string $episode = ("episode_"+$filenamearray[1]);string $scene = ("scene_"+$filenamearray[2]);string $path ="//file-cluster/GDC/Projects/WinxClub/Project/data/"+$episode+"/"+$scene+"/geoCache";//++++++++//+++++++get channel name string $cachefilename = `createNode cacheFile -n tempcacheFile`;  setAttr ".cn" -type "string" ("wi_"+$id+"_"+$chrname+"_h_ms_anim_SLPs");  setAttr ".cp" -type "string" ($path+"/wi_"+$id+"_"+$chrname+"_h_ms_anim") ;string $channelnames[] =`cacheFile -q -cnm  $cachefilename `;delete $cachefilename;//+++++++++++++//+++++++++select import objsselect -cl  ;string $meshesname[] ={};for ($basename in $channelnames){  string $arraych[] ={};  $arraych = stringToStringArray($basename, ":");  int $k =0;  $channelnamespace = "";  for($k=0;$k<size($arraych)-1;$k++)   {    $channelnamespace = ($channelnamespace+$arraych[$k]+":");    }   string $origename = `substitute  $channelnamespace $basename "" `;   $origename = `substitute  "Deformed" $origename "" `;select -add ($namespace+":"+$origename);$meshesname[size($meshesname)] = ($namespace+":"+$origename);}select -r $meshesname;importCacheFile ($path+"/wi_"+$id+"_"+$chrname+"_h_ms_anim/wi_"+$id+"_"+$chrname+"_h_ms_anim_SLPs.xml") "xml";select -cl;}xyFURtool;