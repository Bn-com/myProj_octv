//////////////////////////////////////////////////////////////************************************************************TSM2_ScriptNodesDeletes duplicated script nodes built by TSM2. Only onenodes of each tipe is needed per scene, so you can specifyif you want to delete all these nodes or leave one of themin scene, setting the parameter "$holdFirst" to 0 or 1respectively.int $holdFirst..: 0 to delete oll nodes		    1 to keep one nodes for each type ************************************************************//////////////////////////////////////////////////////////////global proc TSM2_ScriptNodes(int $holdFirst){	delExeptFirst("FKIKSwitch", 0);	delExeptFirst("publishAttrScriptNode", 0);	delExeptFirst("sceneConfigurationScript", 0);	delExeptFirst("uiConfigurationScript", 0);}global proc delExeptFirst(string $stringPart, int $holdFirst){	select -cl;	int $i = 0;	string $listSel[] = `ls`;	for ($obj in $listSel)	{		$cancella = `gmatch $obj ("*" + $stringPart + "*")`;		if ($cancella) 			if (`objExists $obj`)			{				if ($i == 0 && $holdFirst == 0) select -add $obj;				if ($i > 0) select -add $obj;				$i++;			}	}	print("\n sel objs = " + $i + "\n");	doDelete;};