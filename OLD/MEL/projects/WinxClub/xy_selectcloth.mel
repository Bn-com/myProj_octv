global proc xy_selectcloth(){if(`window -exists  xy_selectclothUI`)	deleteUI xy_selectclothUI;window -width 300 xy_selectclothUI;columnLayout -adjustableColumn true;       button -label "bloom_enahantix\\casual\\winter"           -c "selectcloth(\"bloomenahantix\")";    button -label "flora_enahantix"           -c "selectcloth(\"floraenahantix\")";    button -label "layla_enahantix"           -c "selectcloth(\"laylaenahantix\")";    button -label "musa_enahantix"           -c "selectcloth(\"musaenahantix\")";    button -label "stella_enahantix"           -c "selectcloth(\"stellaenahantix\")";     button -label "tecna_enahantix"           -c "selectcloth(\"tecnaenahantix\")";    button -label "mike_standard"           -c "selectcloth(\"mikestandard\")";    button -label "flower_baby_standard"           -c "selectcloth(\"flowerbabystandard\")";    button -label "flower_woman_standard"           -c "selectcloth(\"flowerwomanstandard\")";    button -label "hagen_standard"           -c "selectcloth(\"hagenstandard\")";showWindow xy_selectclothUI;}//++++++++++++++++++++++++++++++++global proc selectcloth(string $chr){string $objname[] = `ls -sl`;string $array[];$array = stringToStringArray($objname[0], ":");int $i;string $namespace = "";for($i=0;$i<size($array)-1;$i++){$namespace = ($namespace+$array[$i]+":");}string $bloomenahantix[]={"MSH_skirt"};string $floraenahantix[]={"MSH_flower_ribbon","MSH_dress"};string $laylaenahantix[]={"leaf_HIres_4","leaf_HIres_3","leaf_HIres_2","leaf_HIres_6","leaf_HIres_5","leaf_HIres_15","leaf_HIres1","leaf_HIres_14","leaf_HIres_13","leaf_HIres_12","leaf_HIres_11","leaf_HIres_10","leaf_HIres_9","leaf_HIres_8","leaf_HIres_7","brassiere_mesh1","skirt_mesh"};string $musaenahantix[]={"_Mesh3","_Meshskirt","_Mesh15"};string $stellaenahantix[]={"skirt02","skirt01"};string $tecnaenahantix[]={"polySurface3000"};string $mikestandard[]={"MSH_Tshirt"};string $flowerbabystandard[]={"MSH_dress"};string $flowerwomanstandard[]={"MSH_cloth"};string $hagenstandard[]={"MSH_cloak","MSH_dress1","MSH_dress2","MSH_dress3"};switch ($chr) {     case "bloomenahantix":         select -cl  ;        for($each in $bloomenahantix)           {           select -add ($namespace+$each);           }         break;    case "floraenahantix":         select -cl  ;        for($each in $floraenahantix)           {           select -add ($namespace+$each);           }         break;     case "laylaenahantix":         select -cl  ;        for($each in $laylaenahantix)           {           select -add ($namespace+$each);           }         break;    case "musaenahantix":         select -cl  ;        for($each in $musaenahantix)           {           select -add ($namespace+$each);           }         break;    case "stellaenahantix":         select -cl  ;        for($each in $stellaenahantix)           {           select -add ($namespace+$each);           }         break;    case "tecnaenahantix":         select -cl  ;        for($each in $tecnaenahantix)           {           select -add ($namespace+$each);           }         break;    case "mikestandard":         select -cl  ;        for($each in $mikestandard)           {           select -add ($namespace+$each);           }         break;    case "flowerbabystandard":         select -cl  ;        for($each in $flowerbabystandard)           {           select -add ($namespace+$each);           }         break;    case "flowerwomanstandard":         select -cl  ;        for($each in $flowerwomanstandard)           {           select -add ($namespace+$each);           }         break;    case "hagenstandard":         select -cl  ;        for($each in $hagenstandard)           {           select -add ($namespace+$each);           }         break;      default:         break; } }xy_selectcloth;