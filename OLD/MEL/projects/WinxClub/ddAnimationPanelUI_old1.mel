global proc slAnimPoseLib_Face_GetPresets()
{
	string $buffer[];

	string $char = `ddAPGetCurrentChar`;
	if ($char == "NONE")
		return ;
	$char = `substring $char 1 (size($char)-1)`;
	
	string $ws = "//file-cluster/GDC/Projects/WinxClub/WinxClub_Scratch/Animation/phonemes";
	string $filename = `referenceQuery  -filename ($char+":Character")`;
	tokenize $filename "/" $buffer;
	tokenize $buffer[size($buffer)-1] "_" $buffer;	
	string $charName = zwGetCharacterNameById($buffer[1]);
	string $path=($ws + "/" + $charName + "/");	
	string $files[] =`getFileList -fld $path -fs  "*.mel"`;
	if (`size $files`)
	{
		string $children[] = `shelfLayout -q -ca ddAPConFaceTabExtra_scr`;
		for ($child in $children)
			deleteUI $child;
	}
	for ($i=0;$i<`size $files`;$i++)
	{
		string $btName=basename($files[$i],".mel");
		shelfButton -annotation $btName -style "iconAndTextVertical" -image1 "commandButton.xpm" -l $btName	-fn "boldLabelFont" -p ddAPConFaceTabExtra_scr
		-command ("source \""+$path+$files[$i]+"\";setPresets(\""+$char+"\");");
	}
}

