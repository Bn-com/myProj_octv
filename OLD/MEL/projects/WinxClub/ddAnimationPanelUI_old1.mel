			//print ("\n------"+$name);
			columnLayout -height 120 -width 100 -p slAnimPoseLibFace_pub_grid;
				symbolButton -ann $name -w 90 -h 90 -i ($path+$image) -c ("slAnimPoseLib_Face_Creat_Key(\""+$name+"\")");
					popupMenu -button 3  ;
						menuItem -en on -label "Remove" -c ("slAnimPoseLib_Face_Remove_Key(\""+$name+"\")") ;
						menuItem -divider true;
						menuItem -en on -label "..." ;
				text -w 90 -h 20 -label $name;
			setParent ..;
		}
	}
	else
	{
		string $childrenArray[] = `gridLayout -q -childArray slAnimPoseLibFace_pub_grid`;
		for ($child in $childrenArray)
			deleteUI  $child;
		
		button -label "NULL" -p  slAnimPoseLibFace_pub_grid;
	}
