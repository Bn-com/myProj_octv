//
//	Arguments : NULL
//		
//	Return :
//		$charName 角色名
//////////////////////////////////////////////////////////
global proc string ddAPGetCurrentChar()
{
	string $charName ;
	$charName = `iconTextRadioCollection -query -select ddAPCharListItRadCollection` ;
	if ($charName == "NONE")
	{
