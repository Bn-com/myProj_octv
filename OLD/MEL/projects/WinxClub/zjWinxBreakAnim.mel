global proc zjWinxBreakAnim(){	string $hairsetup[] = `ls -sl "*:*SETUP*"`;	if (!size($hairsetup))	{		error "ÇëÑ¡ÔñHAIRSETUP×é";	}	string $curves[] = `ls -dag -sl -type nurbsCurve`;	select -r $curves;	pickWalk -d up;	string $sel[] = `ls -l -ni -sl -tr`;	for ($item in $sel)	{		string $cn[] = `listConnections -s 1 -d 0 -type animCurveTL`;		for ($each in $cn)		{			if (`objExists $each`)			deleteIfNotReferenced($each);		}		string $cn[] = `listConnections -s 1 -d 0 -type animCurveTA`;		for ($each in $cn)		{			if (`objExists $each`)			deleteIfNotReferenced($each);		}		string $cn[] = `listConnections -s 1 -d 0 -type animCurveTT`;		for ($each in $cn)		{			if (`objExists $each`)			deleteIfNotReferenced($each);		}		string $cn[] = `listConnections -s 1 -d 0 -type animCurveTU`;		for ($each in $cn)		{			if (`objExists $each`)			deleteIfNotReferenced($each);		}		string $cn[] = `listConnections -s 1 -d 0 -type animCurveUL`;		for ($each in $cn)		{			if (`objExists $each`)			deleteIfNotReferenced($each);		}		string $cn[] = `listConnections -s 1 -d 0 -type animCurveUA`;		for ($each in $cn)		{			if (`objExists $each`)			deleteIfNotReferenced($each);		}		string $cn[] = `listConnections -s 1 -d 0 -type animCurveUT`;		for ($each in $cn)		{			if (`objExists $each`)			deleteIfNotReferenced($each);		}		string $cn[] = `listConnections -s 1 -d 0 -type animCurveUU`;		for ($each in $cn)		{			if (`objExists $each`)			deleteIfNotReferenced($each);		}	}}