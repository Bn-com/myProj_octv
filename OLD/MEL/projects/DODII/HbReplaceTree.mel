p"		120				-attachControl	$lemon1		"left"		5	$blue1				-attachForm		CreatLayerList1		"top"		250				-attachForm	CreatLayerList1			"left"		-140								-attachForm	TheBouuon		"top"		250				-attachControl	TheBouuon			"left"		1  CreatLayerList1							-attachForm	scaleOne		"top"		280				-attachForm	scaleOne			"left"		5					-attachForm	scaleTwo		"top"		280				-attachForm	scaleTwo			"left"		120			$child1;		showWindow HbReplaceTreeWin;}global proc ReplaceTree(string $Type){	print ($Type+"\n");	string $ThePath="Z:/Resource/Support/Maya/projects/DODII/Import/tree/tree"+$Type+"/tree"+$Type+".mb";	string $name=`file -import -type "mayaBinary" -rpr ""  -pr -loadReferenceDepth "all" $ThePath`;	string $GetOlderName=`textFieldGrp -q -text CreatLayerList1`;	string $GetTheOldTreeName[]=`ls $GetOlderName`;	int $i=1;	for ($Obj in $GetTheOldTreeName)	{		$Pos=`xform -q -ws -piv $Obj`;		$rotate=`xform -q -ws -ro $Obj`;		$scale=`xform -q -s $Obj`;		delete $Obj;		string $theCopeTreeName[]=`duplicate -n ("tree"+$Type+"_c_hi_geo"+string($i)) ("tree"+$Type+"_c_hi_geo")`;		xform -ws -t $Pos[0] $Pos[1] $Pos[2] $theCopeTreeName[0];		xform -ws -ro $rotate[0] $rotate[1] $rotate[2] $theCopeTreeName[0];		xform  -s $scale[0] $scale[1] $scale[2] $theCopeTreeName[0];		$i++;		string $QuestTheGroup[]=`ls ("tree"+$Type+"_GRP")`;			if (size($QuestTheGroup)==0)			{string $TheGroupName=`group -em -name ("tree"+$Type+"_GRP")`;								}		parent $theCopeTreeName[0] ("tree"+$Type+"_GRP");			}}