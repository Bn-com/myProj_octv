//////////////////////////////////////////////////////////
// Produces Name: ddCleartext
// Arguments: null
//////////////////////////////////////////////////////////
global proc ddCleartext()
{
	textFieldGrp -edit -text "" fronttext ;
	textFieldGrp -edit -text "" backtext ;
	textFieldGrp -edit -text "" searchtext ;
	textFieldGrp -edit -text "" replacetext ;
}


