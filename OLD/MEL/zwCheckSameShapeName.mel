global proc zwCheckSameShapeName()
{
	string $same[];

	string $buf[] = `ls -shapes`;
	for ($shape in $buf)
	{
		string $name = `match "[^|]+$" $shape`;
		if ($name != $shape)
		{
			int $find = false;
			for ($str in $same)
			{
				if ($str == $name)
				{
					$find = true;
					break;
				}
			}
			if (!$find)
			{
				$same[size($same)] = $name;
				if (size($same) == 1)
				{
					print "重名物体：\n";
				}
				print `ls -long $name`;
				print "\n";
			}
		}
	}
}