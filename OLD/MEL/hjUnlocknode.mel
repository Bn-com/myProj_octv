global proc string hjUnlocknode()
{
	string $jadgnode[]=`ls -type  "shadingEngine"`;
    string $jaunlock;
	for ($jaunlock in $jadgnode)
	{
	lockNode -lock off $jaunlock;	
	}
	return "";
	
}
