global proc zzjDelAllExtraChanls(){//show  MayaManNugget	source "//file-cluster/GDC/Resource/Support/AnimalLogic/mayaman2.0.7/mel/MayaManOptions.mel";	$isBatchMode=`about -batch`;	if (!$isBatchMode)	{		MayaManOptions;		//turn to the extra output channels.	if not the extra channels could not be deleted.        if (!(`separator -q -exists "mayaManOptionsWindow|formLO|thePane|extrachansPane|extrachansCL|extrachansHeader"`))        {			setParent "mayaManOptionsWindow|formLO|thePane|extrachansPane|extrachansCL";			eval "source \"MayaManExtraChannelsGuts.mel\"";		}		paneLayout -e -setPane "extrachansPane" 1 thePane;		menuItem -e -l "MayaMan Extra Output Channels Help" -c "showMayaManHelp(\"extrao\")" helpItem;	}//size the extra channel//	int $no = `scriptTable -q -rows chansList`;	int $no = `MayaManInfo -getarraysize "EOType"` + 1;	for ($i=($no-2);$i>-1;$i--)	{		int $index = $i;////		MayaManAddExtraOutputChannel($chanls[$i], 1, true,0,0,1,0,65535,0,65535,"", 0, 0,"", "","", "");//		MayaManSetData -setstring "EOPath" $extraPaths[$i] ($i);		//	==mmecChangeCell ($i+1) 16 $netRPath;		MayaManSetData -delarrayelement "EOChannel"		$index;		MayaManSetData -delarrayelement "EOEnabled"		$index;		MayaManSetData -delarrayelement "EOType"		$index;		MayaManSetData -delarrayelement "EODoDither"	$index;		MayaManSetData -delarrayelement "EODitherValue"	$index;		MayaManSetData -delarrayelement "EOQuantization"	$index;		MayaManSetData -delarrayelement "EOQuantMin"	$index;		MayaManSetData -delarrayelement "EOQuantMax"	$index;		MayaManSetData -delarrayelement "EOQuantZero"	$index;		MayaManSetData -delarrayelement "EOQuantOne"	$index;		MayaManSetData -delarrayelement "EOFilter"		$index;		MayaManSetData -delarrayelement "EOFilterX"		$index;		MayaManSetData -delarrayelement "EOFilterY"		$index;		MayaManSetData -delarrayelement "EOPath"		$index;		MayaManSetData -delarrayelement "EODriver"		$index;		MayaManSetData -delarrayelement "EODriverArgs"	$index;		MayaManSetData -delarrayelement "EOExt"		$index;		if (!$isBatchMode)		{			scriptTable -e -deleteRow ($index + 1) chansList;		}	}	if (!$isBatchMode)	optionListSelected;}