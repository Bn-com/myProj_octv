global proc button15()
{
string $Char[] = `ls -sl`;

string $CharName[];

int $s = 0;

select -cl;

for ($S in $Char)

{
tokenize $Char[$s] ":" $CharName;

string $Con = $CharName[0]+":hairSystemShape*";

if (objExists ($CharName[0]+":hairSystemShape*"))
{
select -add $Con;
}
else
{
    warning ($S+"==没有选中带控制器的角色");
};
$s=$s+1;
}
};

global proc button16()
{
    string $s[] = `ls "*:hair_con"`;

    for ($S in $s)

    {
        CBdeleteConnection ($S+".v");
        CBdeleteConnection ($S+".HairDynamicCtrlClock");
        CBdeleteConnection ($S+".HairStartFrame");
        CBdeleteConnection ($S+".StartCurveAttact");
        CBdeleteConnection ($S+".Drag");
        CBdeleteConnection ($S+".Damp");
        CBdeleteConnection ($S+".cloth_DynamicCtrlCloak");
        CBdeleteConnection ($S+".cloth_StartFrame");
        CBdeleteConnection ($S+".PrimaryDressCtrl");
        CBdeleteConnection ($S+".SecondaryDressCtrl");
        CBdeleteConnection ($S+".primaryHairCtrl");
        CBdeleteConnection ($S+".SecondaryHairSwitch");
    }
};

global proc button17()
{
string $Char[] = `ls -sl`;

string $CharName[];

int $s = 0;

for ($S in $Char)

{
tokenize $Char[$s] ":" $CharName;

string $Con = $CharName[0]+":hair_con";

if (`attributeExists "HairDynamicCtrlClock" $Con`)
{
setAttr ($Con+".HairDynamicCtrlClock") 0;

setAttr ($Con+".HairStartFrame") 10010;

setAttr ($Con+".primaryHairCtrl") 0;

$s=$s+1;
}

else
{
    warning ($S+"==没有选中带控制器的角色");
};
};


};

