global proc button1()
{
    string $Char[] = `ls -sl`;

    string $CharName[];

    tokenize $Char[0] ":" $CharName;

    string $Con = $CharName[0]+":hair_con";

if (`attributeExists "StartCurveAttact" $Con`)
{
    setAttr ($Con+".StartCurveAttact") 0.1;
}

if (`attributeExists "Drag" $Con`)
{
    setAttr ($Con+".Drag") 0.15;
}

if (`attributeExists "Damp" $Con`)
{
    setAttr ($Con+".Damp") 0.35;
    print "设置好了";
}

else
{
    warning "没有选中带有头部控制器的角色";
}
};

